<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net8.0</TargetFramework>
        <RootNamespace>AssetDownloader</RootNamespace>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AssemblyVersion>8.0</AssemblyVersion>
        <FileVersion>8.0</FileVersion>
        <PackageVersion>7.0.0</PackageVersion>
        <AssemblyName>AssetDownloader</AssemblyName>
        <PackageId>AssetDownloader</PackageId>
        <Authors>AssetDownloader</Authors>
        <ApplicationIcon>Icon\store-logo-booth.ico</ApplicationIcon>
        <IsPackable>false</IsPackable>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="AngleSharp" Version="1.0.7" />
      <PackageReference Include="System.CommandLine" Version="2.0.0-beta4.22272.1" />
      <PackageReference Include="System.Threading.RateLimiting" Version="8.0.0" />
    </ItemGroup>
</Project>
